{
  "name": "druthers",
  "homepage": "https://brennemankyle.github.io/druthers/",
  "version": "3.0.0",
  "description": "React Multi-Select with automatic checkboxes/radios",
  "main": "dist/index.js",
  "private": false,
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.10.5",
    "debounce": "^1.2.1",
    "fuzzyset.js": "1.0.7",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^18.0.2",
    "react-dom": "^18.0.2"
  },
  "scripts": {
    "old-start": "node scripts/start.js",
    "old-build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test-debug": "node --inspect scripts/test.js --runInBand",
    "transpile": "babel ./src --out-dir ./dist",
    "prepare": "npm run transpile",
    "predeploy": "npm run build",
    "start": "start-storybook -p 9009 -s public",
    "build": "build-storybook -s public",
    "deploy": "storybook-to-ghpages"
  },
  "keywords": [
    "multiple select",
    "multi select",
    "multiselect",
    "select",
    "input",
    "react",
    "radio",
    "checkbox",
    "switch",
    "toggle",
    "forms"
  ],
  "author": "Kyle Brenneman <brenneman.kyle@gmail.com>",
  "license": "ISC",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "last 2 edge versions",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@cfaester/enzyme-adapter-react-18": "^0.6.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "@svgr/webpack": "6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^29.4.2",
    "babel-loader": "9.1.2",
    "babel-plugin-named-asset-import": "^0.3.8",
    "camelcase": "^7.0.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "core-js": "^3.27.2",
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "4.2.2",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.33.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.0",
    "fs-extra": "11.1.0",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.2.0",
    "jest": "^29.4.2",
    "jest-emotion": "^11.0.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^29.4.2",
    "jest-watch-typeahead": "^2.2.2",
    "mini-css-extract-plugin": "2.7.2",
    "pnp-webpack-plugin": "1.7.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "7.0.2",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "8.0.1",
    "postcss-safe-parser": "6.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "resolve": "1.22.1",
    "sass-loader": "13.2.0",
    "semver": "7.3.8",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-pnp": "1.2.0",
    "url-loader": "4.1.1",
    "webpack": "5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "4.11.1",
    "webpack-manifest-plugin": "5.0.0",
    "workbox-webpack-plugin": "6.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": {
            "version": 3,
            "proposals": true
          }
        }
      ],
      "@babel/preset-react"
    ]
  }
}
